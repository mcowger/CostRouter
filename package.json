{
  "name": "llm-gateway-main",
  "version": "1.0.0",
  "private": true,
  "description": "An intelligent gateway for routing requests to LLM providers.",
  "type": "module",
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev:server": "tsx watch --inspect server/index.ts  --config ~/workspace/CostRouter/config.jsonc --usage-db-path ~/workspace/CostRouter/usage.db.json",
    "dev:ui": "cd ui && vite",
    "clean": "rm -rf ui/dist && rm -rf server/build && rm -rf server/node_modules && rm -rf ui/node_modules && rm -rf node_modules",
    "setup": "cp config.example.jsonc config.jsonc && echo 'Configuration file created. Please edit config.jsonc with your API keys.'",
    "build-only": "cd ui && vite build",
    "type-check": "vue-tsc --build"
  },
