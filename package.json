{
  "name": "llm-gateway-root",
  "version": "1.0.0",
  "private": true,
  "description": "An intelligent gateway for routing requests to LLM providers.",
  "main": "dist/server/index.js",
  "type": "module",
  "workspaces": [
    "ui"
  ],
  "imports": {
    "#schemas/*": "./dist/schemas/*.js"
  },
  "scripts": {
    "test": "jest",
    "start": "node dist/server/index.js --config config/config.jsonc -l info --usage-db-path config/usage.db.json",
    "test:coverage": "jest --coverage",
    "test:clearCache": "jest --clearCache",
    "test:e2e": "jest --config jest.e2e.config.js",
    "clean": "rm -rf dist node_modules ui/dist ui/node_modules",
    "dev:ui": "npm run dev --workspace=ui",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:ui\"",
    "test:watch": "jest --watch",
    "dev:server": "nodemon",
    "build": "vue-tsc --build && vite --emptyOutDir build ui ",
    "type-check": "vue-tsc --build --noEmit"
  },
  "dependencies": {
    "@ai-sdk/amazon-bedrock": "^2.2.12",
    "@ai-sdk/anthropic": "^1.2.12",
    "@ai-sdk/azure": "^1.3.24",
    "@ai-sdk/deepseek": "^0.2.16",
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/google-vertex": "^2.2.27",
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/mistral": "^1.2.8",
    "@ai-sdk/openai": "^1.3.23",
    "@ai-sdk/openai-compatible": "^0.2.16",
    "@ai-sdk/perplexity": "^1.1.9",
    "@ai-sdk/togetherai": "^0.2.16",
    "@ai-sdk/xai": "^1.2.18",
    "ai": "^4.3.19",
    "ai-sdk-provider-claude-code": "^0.2.2",
    "ai-sdk-provider-gemini-cli": "^0.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "ollama-ai-provider": "^1.2.0",
    "openai": "^5.10.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "qwen-ai-provider": "^0.1.0",
    "rate-limiter-flexible": "^5.0.5",
    "yargs": "^17.7.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "@types/yargs": "^17.0.33",
    "concurrently": "^9.2.0",
    "jest": "^30.0.4",
    "jest-environment-node": "^30.0.4",
    "nodemon": "^3.1.4",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.5.2"
  }
}
